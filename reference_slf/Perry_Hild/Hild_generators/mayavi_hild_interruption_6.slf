tclinit {

            proc CreateFunnelsUI { parent name } {
            set subname "[subst $name]"

            if { $parent == {} } {
              set root .$subname
            } elseif { $parent == "." } {
              set root .$subname
            } else {
              set root $parent.$subname
            }
            
            toplevel $root

    global  f0_r f0_t_r f0_t_u f0_t_n

                label $root.row1Label -text "f0: r, t_r, t_u, t_n"
                scale $root.f0_r -from  1.0 -to 10.0 -orient horizontal -resolution 0.1 -variable f0_r
                set f0_r 6.0
                scale $root.f0_t_r -from  -10.0 -to 10.0 -orient horizontal -resolution 0.1 -variable f0_t_r
                set f0_t_r 0.0
                scale $root.f0_t_u -from  -10.0 -to 10.0 -orient horizontal -resolution 0.1 -variable f0_t_u
                set f0_t_u 0.0
                scale $root.f0_t_n -from  -10.0 -to 10.0 -orient horizontal -resolution 0.1 -variable f0_t_n
                set f0_t_n 0.0

                grid $root.row1Label -row 1 -col 0 -padx 10 -pady 10 -sticky e,w
                grid $root.f0_r -row 1 -col 1
                grid $root.f0_t_r -row 1 -col 2
                grid $root.f0_t_u -row 1 -col 3
                grid $root.f0_t_n -row 1 -col 4

                    wm geometry $root +10+230
                }
                CreateFunnelsUI $winName funs
            }

tclinit {

